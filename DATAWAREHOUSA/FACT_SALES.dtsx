              <properties>
                <property
                  dataType="System.String"
                  description="Spécifie l'instruction SQL utilisée par le composant pour extraire des données."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT

 TC.[DATE_CHARGEMENT]
 ,TC.CODE_CHARGEMENT
 ,TC.[CODE_TOURNEE]
 ,TPC.[CODE_ARTICLE]
 ,[PRIX]
 ,[PRIX_VNT]
 ,SUM([QTE_CHARGEE_POINTE]) as QTE_CHARGEE_POINTE
 ,SUM([QTE_CHARGEE_SUPP]) as QTE_CHARGEE_SUPP
 ,SUM([TOTAL_CHARGEE]) as QTE_CHARGEE_TOTAL
 ,SUM([TOTAL_RENDUS_AG])  as QTE_RENDUS_AG
 ,SUM([TOTAL_RENDUS_US]) as QTE_RENDUS_US
 ,SUM([TOTAL_RENDUS_COM]) as QTE_RENDUS_COM
 ,SUM([TOTAL_RENDUS_SP]) as QTE_RENDUS_SP
 ,SUM([TOTAL_RENDUS_POINTE]) as TOTAL_RENDUS
 ,SUM([QTE_ECART]) as QTE_ECART
 ,SUM([TOTAL_GRATUIT]) as QTE_GRATUIT
 ,SUM(TOTAL_DONS) as QTE_DONS
 ,SUM([TOTAL_REMISE]) as TOTAL_REMISE
 ,SUM([TOTAL_INVENDU_POINTE]) as TOTAL_INVENDU
 ,SUM([CREDIT]) AS TOTAL_VENDU_CAC
 ,SUM([TOTAL_VENDU]) as TOTAL_VENDU_TRAD

  FROM [dbo].[ZAGORA T CHARGEMENT]  AS TC
  INNER JOIN [dbo].[ZAGORA PRODUITS CHARGEE] AS TPC ON TC.[CODE_CHARGEMENT] = TPC.[CODE_CHARGEMENT]

  GROUP BY TC.[DATE_CHARGEMENT], TC.CODE_CHARGEMENT, TC.[CODE_TOURNEE], TPC.[CODE_ARTICLE], [PRIX], [PRIX_VNT]
  ORDER BY TC.[DATE_CHARGEMENT], TC.[CODE_TOURNEE], TPC.[CODE_ARTICLE]</property>
