                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
        DATEADD(dd, 0, DATEDIFF(dd, 0, TP.DATE_HEURE_CREATION)) AS DATE_PROD
        ,TP.DATE_HEURE_CREATION AS HEURE_PROD
        ,TM.CODE_MACHINE
      ,TA.[CODE_ARTICLE]
      ,SUM([NB_UNITES]) AS PROD_U
