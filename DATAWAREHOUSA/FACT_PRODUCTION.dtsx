  INNER JOIN  dbo.T_PALETTE AS TP ON TDP.NUM_PALETTE = TP.NUM_PALETTE
  INNER JOIN dbo.T_MACHINE AS TM ON TM.CODE_MACHINE = TP.MACHINE_SORTIE
